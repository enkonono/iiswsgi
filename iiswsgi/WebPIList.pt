<?xml version="1.0" ?>
<feed xmlns="http://www.w3.org/2005/Atom"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      tal:define="view options/view">
  <webpiFile version="4.0.1.0"
             tal:attributes="version view/webpi_version"/>

  <title tal:content="view/title">WebPI Feed Title</title>
  <link href="https://raw.github.com/rpatterson/iiswsgi/iiswsgi/WebPIList.pt"
        tal:attributes="href view/href"/>
  <updated tal:content="view/now/isoformat">2012-03-25T13:22:48.440870</updated>
  <author>
    <name tal:content="view/author_fullname">Ross Patterson</name>
    <uri  tal:content="view/author_uri">http://rpatterson.net</uri>
  </author>
  <id tal:content="view/id|view/href"
      >https://raw.github.com/rpatterson/iiswsgi/web-pi.xml</id>

  <resourcesList>
    <resources>
      <culture>en</culture>
      <!-- included in file -->
    </resources>
    <tal:comment replace="nothing">
      TODO add other languages
    <resources>
      <culture>de</culture>
      <url>http://www.microsoft.com/web/webpi/3.0/resources/WebApplicationList_de.xml</url>
    </resources>
    </tal:comment>
  </resourcesList>
  <!-- if loaded through a parent XML file then our languages list
       must be a subset of theirs -->
  <languages>
    <language default="true">
      <languageId>en</languageId>
      <name>English</name>
    </language>
  </languages>

<tal:entries tal:repeat="entry view/entries"
             tal:replace="structure entry" />

  <keywords>
    <keyword id="Python" resourceName="Keyword_Python">Python</keyword>
  </keywords>
</feed>
