<?xml version="1.0" encoding="utf-8" ?>
<msDeploy.iisApp>
  <iisApp path="%DIST_NAME%" />
  <runCommand
      path="%SystemDrive%\Python27\Scripts\iiswsgi_install.exe -v -e -a %DIST_NAME% install_msdeploy"
      waitAttempts="10" waitInterval="60000" successReturnCodes="0x0"
      MSDeploy.MSDeployKeyAttributeName="path" />
  <!-- Test the WSGI app -->
  <runCommand
      path="%APPL_PHYSICAL_PATH%\Scripts\paster.exe request -v &quot;%APPL_PHYSICAL_PATH%\__pyramid_project__\development.ini&quot; /"
      successReturnCodes="0x0"
      MSDeploy.MSDeployKeyAttributeName="path" />
</msDeploy.iisApp>
